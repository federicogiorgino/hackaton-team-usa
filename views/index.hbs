<div>
    {{#each data}}
    <a href="/{{this.year}}">{{this.year}}</a>
    {{/each}}
</div>

<div id="main">
    <div id="header" class="section">

        <img class="lax" src="https://media.tenor.com/images/881a6162ac96409feed81258e4471e6d/tenor.gif"
            style="margin-top: -79pt" data-lax-translate-y="0 0, vh 1000" data-lax-opacity="0 1, (0.8*vh) 0" />
        <h2 class="lax" data-lax-scale="0 1, vh 0.2" data-lax-translate-y="0 0, vh 1200"
            data-lax-opacity="0 1, (vh*0.5) 0">American story XX</h2>

        <h4 class="lax" data-lax-opacity="0 1, (vh*0.05) 0">Scroll down to see what happend over the years</h4>
        <i class="lax fas fa-chevron-down" data-lax-opacity="0 1, (vh*0.1) 0" data-lax-translate-y="0 0, 200 100">
        </i>
    </div>

    <!-- begin of loop -->
    {{#each data}}
    <div id="section1" class="section">
        <div class="left">
            <div class="lax bubble a" style="background: #031E67" data-lax-preset="lazy-250"></div>

            <div class="lax bubble c" style="background: #98112C; margin-left: 80pt" data-lax-preset="lazy-100"></div>

            <div class="lax bubble b" style="background: #F8F8F8; margin-left: 160pt" data-lax-preset="lazy-300"></div>

            <h3 data-lax-preset="driftRight" data-lax-optimize=true class="lax chunkyText" style="color: #F8F8F8;"><a
                    href="/{{this.year}}">{{this.year}}</a></h3>
        </div>

        <div class="right">
            <div class="lax bubble a" style="background: #F8F8F8; margin-left: 120pt" data-lax-preset="lazy-200"></div>

            <div class="lax bubble c" style="background: #031E67; margin-left: -20pt" data-lax-preset="lazy-150"></div>

            <div class="lax bubble b" style="background: #98112C; margin-left: 20pt; margin-top: 200pt"
                data-lax-preset="lazy-350"></div>
            <h3 data-lax-optimize=true data-lax-preset="driftLeft" class="lax chunkyText"
                style="color: #98112C; margin-top: 200pt;">{{this.title}}</h3>
        </div>


    </div>

    <div id="section2" class="section">
        <div class="lax blockContainer" data-lax-preset="leftToRight-1.1 fadeInOut">
            <div class="lax block" style="background: #F8F8F8;" data-lax-preset="spin"></div>
        </div>

        <div class="lax blockContainer" data-lax-preset="leftToRight-1.2 fadeInOut">
            <div class="lax block"
                style="background: #031E67; margin-top: -50pt; margin-left: -50pt; width: 40pt; height: 40pt;"
                data-lax-preset="spinRev-500"></div>
        </div>

        <div class="lax blockContainer" data-lax-preset="leftToRight-1.4 fadeInOut">
            <div class="lax block" style="background: #98112C; margin-top: -90pt; margin-left: -0pt;"
                data-lax-preset="spin-500"></div>
        </div>

        <div class="lax blockContainer" data-lax-preset="leftToRight-1.5 fadeInOut">
            <div class="lax block"
                style="background: #031E67; margin-top: 70pt; margin-left: -150pt; width: 40pt; height: 40pt;"
                data-lax-preset="spinRev-500"></div>
        </div>

        <div class="lax blockContainer" data-lax-preset="leftToRight-1.3 fadeInOut">
            <div class="lax block"
                style="background: #031E67; margin-top: 100pt; margin-left: -60pt; width: 25pt; height: 25pt;"
                data-lax-preset="spin-500"></div>
        </div>

        <div class="lax blockContainer" data-lax-preset="leftToRight-1.05 fadeInOut">
            <div class="lax block" style="background: #98112C; margin-top: -30pt; margin-left: -70pt;"
                data-lax-preset="spin"></div>
        </div>

        {{!-- <h3 data-lax-preset="leftToRight-0.8 speedy" data-lax-optimize=true class="lax chunkyText" style="
          color: #white; position: absolute; margin-top: -20pt; margin-left: -100pt">
          Some interesting facts about taht year...
        </h3>  --}}

        <div class="lax blockContainer" data-lax-preset="leftToRight-1.15 fadeInOut">
            <div class="lax block"
                style="background: #F8F8F8; margin-top: -70pt; margin-left: -20pt; width: 40pt; height: 40pt;"
                data-lax-preset="spinRev-500"></div>
        </div>

        <div class="lax blockContainer" data-lax-preset="leftToRight-1.45 fadeInOut">
            <div class="lax block"
                style="background: #98112C; margin-top: -50pt; margin-left: -50pt; width: 25pt; height: 25pt;"
                data-lax-preset="spin-500"></div>
        </div>

        <div class="lax blockContainer" data-lax-preset="leftToRight-1.5 fadeInOut">
            <div class="lax block" style="background: #F8F8F8; margin-top: 30pt; margin-left: -20pt;"
                data-lax-preset="spinRev-500"></div>
        </div>

        <div class="lax blockContainer" data-lax-preset="leftToRight-1.25 fadeInOut">
            <div class="lax block" style="background: #98112C; margin-top: 80pt; margin-left: -10pt;"
                data-lax-preset="spin-500"></div>
        </div>
    </div>

    {{/each}}




</div>